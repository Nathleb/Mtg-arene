{
  "name": "Node-expressAPI",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && cd ./client && rm -rf build",
    "build-babel": "babel -d ./build ./src -s",
    "build-server": "npm run build-babel",
    "build-client": "npm run build --prefix client",
    "build": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "fbuild": "concurrently \"npm run clean\" \"npm run build-server\"",
    "client-install": "npm install --prefix client",
    "heroku-postbuild": "npm run client-install && npm run build-client",
    "start": "node ./build/index.js",
    "server": "nodemon --exec babel-node src/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.15.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.7",
    "mongoose-simple-random": "^0.4.1",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.15.8",
    "nodemon": "^2.0.12"
  },
  "engines": {
    "node": "14.18.1"
  }
}